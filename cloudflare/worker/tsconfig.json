{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "isolatedModules": true,
        "allowJs": false,
        "lib": [
            "ES2022"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "noEmit": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx"
    },
    "include": [
        "src/**/*"
    ]
}