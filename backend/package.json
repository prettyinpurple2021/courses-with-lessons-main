    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.6",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
